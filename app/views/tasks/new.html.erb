<h1>Tasks#new</h1>
<p>Find me in app/views/tasks/new.html.erb</p>


<%= form_for(@task) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :completed %>
  <%= f.text_field :completed %>

  <%= f.label :pay %>
  <%= f.text_field :pay %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.submit %>
<% end %>
